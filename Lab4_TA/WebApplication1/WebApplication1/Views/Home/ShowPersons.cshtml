@{
}
@model Persons[]
<table class="table">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Surname</td>
        <td>Age</td>
    </tr>
    @foreach(var p in Model)
    {
        <tr>
            <td>@p.Id</td>
            <td>@p.Name</td>
            <td>@p.Surname</td>
            <td>@p.Age</td>
        </tr>
    }
</table>